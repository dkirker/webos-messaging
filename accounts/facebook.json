{
    "name": "chat",
    "basename": "facebook",
    "servicename": "type_facebook",
    
    "@include": ["jabber.json"],

    "@icons": {
        "icon32": {
            "path": "$icon_path/48/$iconname.png"
        }
    },

    "@template": {
        "loc_name": "Facebook Chat",
        "loc_usernameLabel": "Facebook E-Mail",
        "preferences": {
            "server": "chat.facebook.com",
            "port": 5222,
            "auth_plain_in_clear": false,
            "connection_security": "opportunistic_tls"
        }
    }
}
